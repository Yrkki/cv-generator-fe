{
  "extends": [
    "stylelint-config-standard-scss"
  ],
  "plugins": [
    "stylelint-order"
  ],
  "rules": {
    "order/order": [
      "dollar-variables",
      "custom-properties",
      "at-rules",
      "less-mixins",
      "declarations",
      "rules"
    ],
    "declaration-empty-line-before": "never",
    "scss/percent-placeholder-pattern": null
  }
}