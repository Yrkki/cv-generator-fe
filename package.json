{
  "name": "cv-generator-fe",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node server.js --env=heroku",
    "build": "ng build --prod --env=heroku",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "manifest-copy": "cp src/manifest.json dist/",
    "build-prod-ngsw": "ng build --prod && npm run ngsw-config && npm run ngsw-copy && npm run manifest-copy",
    "serve-prod-ngsw": "npm run build-prod-ngsw && http-server dist -p 5000",
    "postinstall": "npm run build-prod-ngsw",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.9",
    "@angular/cli": "1.7.4",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "5.2.9",
    "@angular/service-worker": "^5.2.9",
    "bootstrap": "^4.1.0",
    "chart.js": "^2.7.2",
    "core-js": "^2.5.5",
    "express": "^4.16.3",
    "ng-pwa-tools": "0.0.15",
    "rxjs": "^5.5.9",
    "tslint-angular": "^1.1.1",
    "typescript": "2.6.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "5.2.9",
    "@angular/language-service": "5.2.9",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.104",
    "codelyzer": "^4.0.1",
    "enhanced-resolve": "^3.4.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "^5.0.1",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.7.1"
  }
}
