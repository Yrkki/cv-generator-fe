{
  "name": "cv-generator-fe",
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ver": "ng version && echo Node, NPM and NVM version: && node -v && npm -v && nvm v",
    "start": "node server.js",
    "build": "ng build --prod",
    "build:staging": "ng build --prod --configuration=heroku",
    "compodoc": "compodoc -p src/tsconfig.app.json --theme vagrant --hideGenerator --disableSourceCode --disablePrivate --disableTemplateTab --customFavicon \"./src/favicon/android-chrome-512x512.png\" -n \"CV Generator Documentation\"",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "favicon-copy": "cp src/favicon/* dist/favicon/",
    "doc-copy": "cp -r documentation dist/documentation/",
    "cov-copy": "cp -r coverage dist/coverage/ && npm run coverage-report",
    "google-copy": "cp src/google/*.* dist/",
    "manifest-copy": "cp src/manifest.json dist/",
    "cov": "npm run test-once && npm run posttest",
    "doc": "npm run compodoc && npm run doc-copy",
    "build-prod-ngsw": "npm run build",
    "build-prod-ngsw:staging": "npm run build:staging",
    "postbuild": "npm run ngsw-config && npm run ngsw-copy && npm run favicon-copy && npm run google-copy && npm run manifest-copy && npm run doc",
    "postbuild:staging": "npm run postbuild",
    "serve-prod-ngsw": "npm run build-prod-ngsw && http-server dist -p 5000",
    "postinstall": "npm run build-prod-ngsw:staging",
    "wake-up-the-dynos": "curl cvgenerator.ml",
    "vulnerability-check": "snyk test",
    "test": "npm run vulnerability-check && ng test --code-coverage",
    "test-once": "env singleRun=true ng test --code-coverage",
    "posttest": "npm run cov-copy",
    "docker-build": "docker image build -t jorich/cv-generator-fe:%npm_package_version% -t jorich/cv-generator-fe .",
    "docker-push": "docker push jorich/cv-generator-fe:%npm_package_version% && docker push jorich/cv-generator-fe:latest",
    "dockerize": "npm run docker-build && npm run docker-push",
    "pack:staging": "npm run build-prod-ngsw:staging && npm run cov && npm run dockerize",
    "deploy": "cd ../cv-generator-life-terraform && terraform apply",
    "codecov-report": "codecov",
    "coveralls-report": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "coverage-report": "npm run codecov-report && npm run coveralls-report",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cli": "^9.1.7",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "9.1.9",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/platform-server": "^9.1.9",
    "@angular/pwa": "^0.901.7",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@compodoc/compodoc": "^1.1.11",
    "chart.js": "2.9.3",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "plotly.js": "^1.54.1",
    "rxjs": "^6.5.5",
    "snyk": "^1.336.0",
    "tslint-angular": "^3.0.2",
    "typescript": "3.8.3",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.7",
    "@angular/language-service": "9.1.9",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.0.11",
    "auto-changelog": "^2.0.0",
    "codecov": "^3.7.0",
    "codelyzer": "5.2.2",
    "coveralls": "^3.1.0",
    "enhanced-resolve": "^4.1.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "5.0.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "node-sass": "^4.14.1",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "tslint": "^6.1.2"
  },
  "engines": {
    "node": "14.3.0",
    "npm": "6.14.5"
  },
  "snyk": true
}
